{
  "rewrites": [
    { "source": "/", "destination": "/src/pages/index.html" },
    { "source": "/api/listar", "destination": "/api/listar.js" }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET" }
      ]
    }
  ]
}
